#
# Trigger a callstack overflow recursively calling a  method that takes no
# arguments.
#

$modname = FailCallStackOverflow;

method start<> void {

$limstack = 0;
$limlocal = 0;
    
    call looparound;
    leave;
}


method looparound<> void {

$limstack = 1;
$limlocal = 0;

    ldsc "In loop!";
    call csm:io_print_strconst;
    call looparound;
    leave;
}
